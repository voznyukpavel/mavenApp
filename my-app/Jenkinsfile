
def server = Artifactory.server('jfrog')

def uploadSpec = """{
  "files": [
    {
      "pattern": "my-app/target/**.jar",
      "target": "Jenkins-integration/"
    }
 ]
}"""

pipeline {
agent any
 stages{
	stage('Build') {  
      
      steps {               
             bat'compile'
   		     echo 'Building..'           
           }        
      }     
      stage('Test') {          
        steps {       
        	bat'make check || true'
        	bat'test'
            echo 'Testing..'           
           }        
       }       
       stage('Deploy') {    
       	 when{              
	        expression {
	                  currentBuild.result == null || currentBuild.result == 'SUCCESS'
	                 }
	         }         
           steps {               
            echo 'Deploying....'
            bat'install'
        }  
      }
       stage('My stage') {           
           steps {               
            echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}" 
            echo 'My stage....'           
        }     
    }
      stage('END') {
      node {
          echo 'Hello Artifactory'
          server.upload(uploadSpec)
      }
           steps {              
            echo 'Built....'        
        }  
    }
  }
}
 

